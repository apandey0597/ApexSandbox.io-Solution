public Boolean sameElements(List<Integer> nums1, List<Integer> nums2) {

   boolean flg = true;
   for(Integer i : nums1){
       if(!nums2.contains(i))
       flg = false;
   }
   boolean flg2 = true;
   for(Integer i : nums2){
       if(!nums1.contains(i))
       flg = false;
   }
    return flg;
}