public Boolean sameElements(List<Integer> nums1, List<Integer> nums2) {

   boolean flg = false;
   /*for(Integer i : nums1){
       if(!nums2.contains(i))
       flg = false;
   }
   boolean flg2 = true;
   for(Integer i : nums2){
       if(!nums1.contains(i))
       flg = false;
   }*/

   Set<Integer> s1 = new Set<Integer>(nums1);
   Set<Integer> s2 = new Set<Integer>(nums2);
   flg = s1.containsAll(s2);
    return flg;
}