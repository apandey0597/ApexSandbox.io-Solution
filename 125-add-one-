public void plusOne(List<Integer> numbers) {
    // code here
    Integer size = numbers.size();
    while(size > 0){

       Integer last = numbers.get(size-1);
       Integer penultimate = numbers.get(size-2);
       ///
       if(last<9){
           numbers.set(size-1,(last+1));
       }else if(last==9){
           if(penultimate<9){
                 numbers.set(size-1,0);
                 numbers.set(size-2, penultimate+1);

           }
       }
       ///
       size--;
    }
}